APP_IDENTIFIER=org.reactjs.native.example.reactRaj
APP_NAME=reactRaj
PRODUCE_VERSION=0.1.0
APP_VERSION=$PRODUCE_VERSION
PRODUCE_SKU=reactRaj_001
PRODUCE_PLATFORMS=ios,macos
APP_PROJECT_PATH=reactRaj.xcodeproj
