FL_CODE_SIGN_IDENTITY=Apple Distribution
FL_PROJECT_SIGNING_PROJECT_PATH=$APP_PROJECT_PATH

#build
GYM_SCHEME=reactRaj
#or appconfigureation
GYM_EXPORT_METHOD="app-store"
GYM_OUTPUT_DIRECTORY=ios/build
